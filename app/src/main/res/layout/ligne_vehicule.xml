<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:id="@+id/id_vehicule"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Id vehicule"/>


        <TextView
            android:id="@+id/libelle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Libelle vehicule"/>

        <TextView
            android:id="@+id/nb_place"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Nombre place"/>

        <TextView
            android:id="@+id/location_min"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Location Min"/>

        <TextView
            android:id="@+id/location_max"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Location Max"/>

        <TextView
            android:id="@+id/tarif_min"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Tarif Min"/>


        <TextView
            android:id="@+id/tarif_max"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Tarif Max"/>

    </LinearLayout>

</LinearLayout>